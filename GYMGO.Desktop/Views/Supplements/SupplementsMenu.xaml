<UserControl x:Class="GYMGO.Desktop.Views.Supplements.SupplementsMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GYMGO.Desktop.Views.Supplements" xmlns:supplements="clr-namespace:GYMGO.Desktop.ViewModels.Supplements" d:DataContext="{d:DesignInstance Type=supplements:SupplementsViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="800">
    <Border>
        <StackPanel Grid.Row="1" Orientation="Horizontal">
            <Button 
        Style="{StaticResource subMenuButton}"
        HorizontalAlignment="Left"
        Width="150"
        Margin="235 0 0 0"
        Content="Táplálékkiegészítők"
        Command="{Binding ShowNutrionalViewCommand}"
    />
            <Button 
        Style="{StaticResource subMenuButton}"
        HorizontalAlignment="Left"
        Width="150"
        Margin="12 0 0 0"
        Content="Felszerelés"
        Command="{Binding ShowEquipmentlViewCommand}"
    />
        </StackPanel>
    </Border>
</UserControl>
