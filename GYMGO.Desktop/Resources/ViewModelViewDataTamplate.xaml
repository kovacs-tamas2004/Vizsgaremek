<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controlPanelViewModels="clr-namespace:GYMGO.Desktop.ViewModels.ControlPanel"
                    xmlns:controlPanelView="clr-namespace:GYMGO.Desktop.Views.ControlPanel"
                    xmlns:usersViewModels="clr-namespace:GYMGO.Desktop.ViewModels.Users"
                    xmlns:usersView="clr-namespace:GYMGO.Desktop.Views.Users">
                    
    <DataTemplate DataType="{x:Type controlPanelViewModels:ControlPanelViewModel}">
        <controlPanelView:ControlPanelView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type usersViewModels:UsersViewModel}">
        <usersView:UsersView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type usersViewModels:VisitorsViewModel}">
        <usersView:VisitorsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type usersViewModels:TrainersViewModel}">
        <usersView:Trainers/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type usersViewModels:OwnersViewModel}">
        <usersView:Owners/>
    </DataTemplate>
</ResourceDictionary>